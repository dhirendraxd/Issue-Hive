{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/((?!assets|favicon.ico|manifest.json|beehive-honey-svgrepo-com.svg|placeholder.svg).*)",
      "destination": "/index.html"
    }
  ]
}
